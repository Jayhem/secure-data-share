{
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
      "include": ["src"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["node_modules/markdown/lib/markdown", "node_modules/better-docs/component"],
    "templates": {
      "better-docs": {
        "name": "My React components"
      }
    },
    "opts": {
      "template": "node_modules/better-docs",
      "destination": "docs",
      "recurse": true,
      "readme": "README.md"
    }
  }